{
    "name": "rest-control/rest-control",
    "type": "library",
    "description": "PHP Framework for testing and validation REST services",
    "keywords": ["framework", "testing", "rest", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Szela",
            "email": "kamil.szela@cothe.pl"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "league/container": "^2.4",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/property-access": "^3.4",
        "doctrine/annotations": "^1.4",
        "league/pipeline": "^0.3.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/finder": "^4.0",
        "psr/log": "^1.0"
    },
    "bin": ["bin/rest-control"],
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
      "psr-4": {
        "RestControl\\": "src//"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "RestControl\\Tests\\": "tests//"
      }
    }
}
